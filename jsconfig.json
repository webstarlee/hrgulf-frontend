{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "baseUrl": "src",
    "paths": {
      "@/*": ["src/*"],
      "@src/*": ["src/*"],
      "@actions/*": ["src/actions/*"],
      "@apis/*": ["src/apis/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@core/*": ["src/core/*"],
      "@helpers/*": ["src/helpers/*"],
      "@pages/*": ["src/pages/*"],
      "@reducers/*": ["src/reducers/*"],
      "@services/*": ["src/services/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}